{
  "name": "hotel-management-system",
  "version": "1.0.0",
  "description": "A comprehensive web application for hotel management with LangGraph multi-agent AI",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "setup-and-start": "npm run install:all && npm start",
    "test": "concurrently \"cd backend && npm test\" \"cd frontend && npm test\"",
    "setup:chatbot": "python setup_chatbot.py",
    "test:gpu": "python test_gpu_and_model.py",
    "start:langgraph": "node start_langgraph_system.js",
    "setup:langgraph": "cd backend/ai_agents && pip install -r requirements.txt",
    "setup-and-start:langgraph": "npm run install:all && npm run setup:langgraph && npm run start:langgraph"
  },
  "keywords": [
    "hotel",
    "management",
    "react",
    "node",
    "express",
    "sqlite",
    "langgraph",
    "ai",
    "multi-agent"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.17"
  },
  "dependencies": {
    "all": "^0.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "uuid": "^9.0.1"
  }
}
